        <Combinator xsi:type="rx:CombineLatest" />
      </Expression>
      <Expression xsi:type="scr:ExpressionTransform">
        <scr:Expression>new (
  it.Item1 as Cameraframes,
  it.Item2 as Quinine,
  it.Item3 as Airpuff,
  it.Item4 as Sucrose
)</scr:Expression>
      </Expression>
      <Expression xsi:type="io:CsvWriter">
